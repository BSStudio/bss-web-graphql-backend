{
  "$schema": "https://www.schemastore.org/semantic-release.json",
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
        "@semantic-release/release-notes-generator",
        {
          "presetConfig": {
            "types": [
              { "type": "chore", "section": "Chores" }
            ]
          }
        }
    ],
    "@semantic-release/github"
  ]
}
